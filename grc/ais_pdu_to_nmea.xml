<?xml version="1.0"?>
<block>
  <name>pdu_to_nmea</name>
  <key>ais_pdu_to_nmea</key>
  <category>[ais]</category>
  <import>import ais</import>
  <make>ais.pdu_to_nmea($designator)</make>

  <param>
    <name>Designator</name>
    <key>designator</key>
    <type>string</type>
  </param>

  <sink>
    <name>print</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

  <sink>
    <name>to_nmea</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

  <source>
    <name>out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
